# Solana RPC endpoint (use a private RPC for production)
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Wallet private key (base58 or JSON array)
# WARNING: Never commit real keys! Use environment variables or secure storage.
WALLET_PRIVATE_KEY=

# Risk tolerance: conservative, moderate, aggressive
RISK_TOLERANCE=moderate

# Minimum APY improvement to trigger rebalance (percentage)
REBALANCE_THRESHOLD=0.5

# Maximum slippage for swaps (percentage)
MAX_SLIPPAGE=0.5

# Dry run mode: true = simulate, false = execute real trades
DRY_RUN=true
